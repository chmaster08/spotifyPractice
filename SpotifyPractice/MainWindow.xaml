        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SpotifyPractice"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Vertical">
            <TextBlock x:Name="Test" FontSize="20" VerticalAlignment="Center"></TextBlock>
            <Button Content="Click Me" Click="Click_Button" VerticalAlignment="Bottom" HorizontalAlignment="Center"/>
        </StackPanel>
        
    </Grid>
</Window>
