<Window x:Class="SpotifyPractice.testwindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SpotifyPractice"
        mc:Ignorable="d"
        Title="testwindow" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Vertical">
            <TextBlock Width="100" Height="100" Text="{Binding Path=Response}" FontSize="20"/>
            <Button Content="Get Request" HorizontalAlignment="Center" Command="{Binding Path=GetCommand}"/>
        </StackPanel>
    </Grid>
</Window>
